<h1>Products</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.title %></td>
        <td><%= product.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Product', new_product_path %>

<%= button_to "Logout", destroy_user_session_path, method: :delete, class: "btn btn-danger ml-2",style:"margin: 5px;" %>
